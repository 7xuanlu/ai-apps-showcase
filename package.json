{
  "name": "ai-apps-showcase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:generate-schema": "node scripts/generate-schema.js",
    "db:generate-schema:sqlite": "cross-env DATABASE_PROVIDER=sqlite node scripts/generate-schema.js",
    "db:generate-schema:postgresql": "cross-env DATABASE_PROVIDER=postgresql node scripts/generate-schema.js",
    "prisma:migrate:dev": "yarn prisma migrate dev",
    "prisma:migrate:deploy": "yarn prisma migrate deploy",
    "prisma:studio": "yarn prisma studio",
    "db:init": "node scripts/init-database.js",
    "db:init:dev": "cross-env NODE_ENV=development node scripts/init-database.js",
    "db:init:prod": "cross-env NODE_ENV=production node scripts/init-database.js",
    "db:migrate": "node scripts/migration-utils.js migrate",
    "db:migrate:dev": "cross-env NODE_ENV=development node scripts/migration-utils.js migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production node scripts/migration-utils.js migrate",
    "db:reset": "node scripts/migration-utils.js reset",
    "db:reset:dev": "cross-env NODE_ENV=development node scripts/migration-utils.js reset",
    "db:seed:dev": "cross-env NODE_ENV=development tsx prisma/seed.ts",
    "db:seed:prod": "cross-env NODE_ENV=production tsx prisma/seed.ts",
    "db:validate": "node scripts/test-db-connection.js",
    "db:status": "node scripts/migration-utils.js detect",
    "db:create-migration": "node scripts/migration-utils.js create-migration",
    "db:setup": "yarn db:generate-schema && yarn db:init && yarn db:seed",
    "db:setup:dev": "cross-env NODE_ENV=development yarn db:setup",
    "db:setup:sqlite": "cross-env DATABASE_PROVIDER=sqlite NODE_ENV=development yarn db:setup",
    "db:setup:postgresql": "cross-env DATABASE_PROVIDER=postgresql NODE_ENV=production yarn db:setup",
    "db:fresh": "yarn db:reset && yarn db:setup",
    "db:fresh:dev": "cross-env NODE_ENV=development yarn db:fresh",
    "env:validate": "node scripts/validate-environment.js",
    "env:validate:dev": "cross-env NODE_ENV=development node scripts/validate-environment.js",
    "env:validate:prod": "cross-env NODE_ENV=production node scripts/validate-environment.js",
    "env:test": "node scripts/test-env-validation.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^8.2.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.44.1",
    "next": "14.1.0",
    "next-auth": "4.24.5",
    "node-fetch": "^2.6.0",
    "prisma": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-social-icons": "^6.24.0",
    "tailwindcss": "^4.1.10",
    "uuid": "^7.0.3",
    "xmlbuilder": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "24.0.1",
    "@types/react": "19.1.8",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.5.5",
    "tsx": "^4.20.3",
    "typescript": "5.8.3"
  }
}
